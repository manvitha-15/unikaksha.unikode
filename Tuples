#add an item
tp=(4,6,2,8,3,1)
print(tp)
x=(9,)
tp=tp+x
print(tp)
tp=tp[:5]+(15,20,25)+tp[:5]
print(tp)
list=list(tp)
list.append(30)
tp=tuple(list)
print(tp)

#length of a tuple
n=input()
a=tuple(n)
print(a)
c=len(n)
print(c)

#reverse of a tuple
n=input()
n=tuple(n)
print(n[::-1])

#repeated element in a tuple
tp=(2,4,5,6,2,3,4,4,7)
print(tp)
n=int(input())
print(tp.count(n))

